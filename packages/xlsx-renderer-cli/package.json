{
  "name": "sxr",
  "version": "0.0.8",
  "license": "MIT",
  "description": "Command line tool for generating XLSX files from a template. Part of XToolset",
  "main": "lib/program.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Siemienik/XToolset",
    "directory": "packages/xlsx-renderer-cli"
  },
  "keywords": [
    "xlsx",
    "cli",
    "excel",
    "template",
    "render",
    "generator",
    "view model",
    "spreadsheet",
    "sheet",
    "view engine",
    "xlsx view engine"
  ],
  "author": "Pawe≈Ç Siemienik <npm@siemienik.pl>",
  "scripts": {
    "build": "rm -rf lib/* && tsc",
    "format": "npm run format:code && npm run format:markdown",
    "format:code": "prettier --write \"**/*.{js,jsx,ts,vue}\"",
    "format:markdown": "markdownlint --fix **/*.md",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "tslint --fix -p tsconfig.json",
    "prepublishOnly": "npm run lint && npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A ./sxr.ts ./program.ts"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/node": "^14.14.25",
    "markdownlint-cli": "^0.31.0",
    "prettier": "^1.17.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.4"
  },
  "dependencies": {
    "commander": "^6.2.1",
    "xlsx-renderer": "^2.4.2"
  },
  "files": [
    "lib/*.js",
    "lib/*.d.ts",
    "README.md",
    "LICENSE"
  ],
  "types": "lib/program.d.ts",
  "bin": {
    "sxr": "lib/sxr.js"
  },
  "funding": {
    "url": "https://github.com/sponsors/Siemienik"
  }
}
